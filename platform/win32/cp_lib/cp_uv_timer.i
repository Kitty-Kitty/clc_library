int cp_uv_timer_init(cp_uv_timer_t* th){
	cp_uv_timer_data_t *s;

	s = (cp_uv_timer_data_t*)malloc(sizeof(cp_uv_timer_data_t));

	s->loop = (uv_loop_t *)malloc(sizeof(uv_loop_t));

	uv_loop_init(s->loop);

	th->s = s;

	uv_timer_init(s->loop, &(s->u_timer));

	return 0;
}

int cp_uv_timer_start(cp_uv_timer_t* th,
	cp_timer_cb cb,
	cp_uint64_t timeout,
	cp_uint64_t repeat){
	cp_uv_timer_data_t *data;

	data = (cp_uv_timer_data_t*)th->s;

	uv_timer_start(&(data->u_timer), (uv_timer_cb)cb, timeout, repeat);

	uv_run(data->loop, UV_RUN_DEFAULT);

	return 0;
}

int cp_uv_timer_stop(cp_uv_timer_t* th){
	cp_uv_timer_data_t *data;
	int ret;

	data = (cp_uv_timer_data_t*)th->s;

	ret = uv_timer_stop(&(data->u_timer));

	if (data->loop){
		uv_loop_close(data->loop);

		free(data->loop);
	}

	if (th->s){
		free(th->s);
	}

	return ret;
}

int cp_uv_timer_again(cp_uv_timer_t* th){
	cp_uv_timer_data_t *data;

	data = (cp_uv_timer_data_t*)th->s;

	return uv_timer_again(&(data->u_timer));
}

void cp_uv_timer_set_repeat(cp_uv_timer_t* th, cp_uint64_t repeat){
	cp_uv_timer_data_t *data;

	data = (cp_uv_timer_data_t*)th->s;

	uv_timer_set_repeat(&(data->u_timer), repeat);
}

cp_uint64_t cp_uv_timer_get_repeat(const cp_uv_timer_t* th){
	cp_uv_timer_data_t *data;

	data = (cp_uv_timer_data_t*)th->s;

	return uv_timer_get_repeat(&(data->u_timer));
}
